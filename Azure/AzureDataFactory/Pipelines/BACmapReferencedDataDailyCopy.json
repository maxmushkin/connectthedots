{
  "name": "BACmapReferencedDataDailyCopy",
  "properties": {
    "description": "Copy reference data from a Azure SQL table to blob",
    "activities": [
      {
        "type": "Copy",
        "typeProperties": {
          "source": {
            "type": "SqlSource"
          },
          "sink": {
            "type": "BlobSink",
            "blobWriterAddHeader": true,
            "writeBatchSize": 0,
            "writeBatchTimeout": "00:00:00"
          }
        },
        "inputs": [
          {
            "name": "AzureSQLBACmapDailyTemplate"
          }
        ],
        "outputs": [
          {
            "name": "AzureBlobBACmapDailyTemplate"
          }
        ],
        "policy": {
          "timeout": "01:00:00",
          "concurrency": 1,
          "executionPriorityOrder": "NewestFirst",
          "style": "StartOfInterval"
        },
        "scheduler": {
          "frequency": "Day",
          "interval": 1
        },
        "name": "CopyFromSQLToBlob",
        "description": "Copy reference customer data from a Azure SQL table to blob"
      }
    ],
    "start": "2017-01-01T00:00:00Z",
    "end": "2018-01-01T00:00:00Z",
    "isPaused": false,
    "pipelineMode": "Scheduled"
  },
  "$schema": "http://datafactories.schema.management.azure.com/internalschemas/2015-09-01/Microsoft.DataFactory.Pipeline.json"
}